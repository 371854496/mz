div.home-banner {
  position: relative;
  height: 680px;
  background: black;
}

div.banner-container { /**banner¿ò**/
  position: relative;
  height: 100%;
  width: 100%;
  overflow: hidden;
}

ul.banner-pic {
  position: relative;
  height: 100%;
  top: 0;
  left: 0;
  border: 0;
  /*	transition:all 0.5s ease-out;*/
}

ul.banner-pic:after {
  content: "";
  display: block;
  clear: both;
}

ul.banner-pic li {
  height: 680px;
  float: left;
  position: relative;
  overflow: hidden;
}

ul.banner-pic li img {
  height: 100%;
  position: absolute;
  left: 50%;
  top: 0;
  transform: translateX(-50%);
}

/**ÂÖ²¥Ð¡Ô²µã**/
ul.pagination {
  width: 500px;
  padding: 0 10px;
  height: 20px;
  position: absolute;
  left: 50%;
  transform: translate(-50%);
  bottom: 10px;
  text-align: center;
  /*	border:red 1px solid;*/
  display: flex;
  justify-content: center;
}

ul.pagination li {
  margin: 3px 12px;
  width: 10px;
  height: 14px;
  background-repeat: no-repeat;
  background-position: -14px center;
  cursor: pointer;
  background-image: url(../imgs/HOME/banner/icon-circle_61b7196.png);
}

ul.pagination li.active {
  width: 14px;
  background-position: 0 center;
}

